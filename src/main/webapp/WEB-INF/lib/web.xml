<servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>com.trail.loginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>RegistrationServlet</servlet-name>
    <servlet-class>com.trail.registrationservlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DownloadServlet</servlet-name>
    <servlet-class>com.trail.pdfdownload.servlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RegistrationServlet</servlet-name>
    <url-pattern>/register</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>/PdfServlet</servlet-name>
    <url-pattern>/download</url-pattern>
  </servlet-mapping>
